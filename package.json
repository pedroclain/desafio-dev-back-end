{
  "name": "desafio-dev-back-end",
  "version": "1.0.0",
  "description": "Precato. Desafio: Desenvolvedor back-end, por Pedro Henrique Clain",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "dev": "ts-node-dev --inspect --transpile-only --ignore-watch node_modules src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc"
  },
  "jest": {
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.json"
      }
    },
    "testMatch": [
      "**/__tests__/*.+(js)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pedroclain/desafio-dev-back-end.git"
  },
  "author": "Pedro Henrique Clain",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pedroclain/desafio-dev-back-end/issues"
  },
  "homepage": "https://github.com/pedroclain/desafio-dev-back-end#readme",
  "dependencies": {
    "express": "^4.18.1",
    "jest": "^28.1.0",
    "knex": "^2.1.0",
    "mysql2": "^2.3.3",
    "node-schedule": "^2.1.0",
    "supertest": "^6.2.3",
    "ts-jest": "^28.0.4",
    "tslog": "^3.3.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^28.1.1",
    "@types/node-schedule": "^2.1.0",
    "@types/supertest": "^2.0.12",
    "ts-node-dev": "^2.0.0"
  }
}
